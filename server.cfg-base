echo Initializing LVP Production server...

announce 1
chatlogging 1
lanmode 0
rcon 1

logqueries 1

maxplayers 208
maxnpc 20

conncookies 0
die_on_error 0
lagcompmode 1

language All
port ${PORT}
rcon_password ${RCON_PASSWORD}
weburl sa-mp.nl

gamemode0 lvp 1
filterscripts cake drop_radio
plugins playgroundjs-plugin.so echo-plugin.so json-plugin.so mysql-plugin.so zone-manager-plugin.so streamer.so
hostname ${TITLE}

logtimeformat [%d/%m %H:%M:%S]
