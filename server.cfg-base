weburl sa-mp.nl

gamemode0 lvp 1
plugins playgroundjs-plugin.so echo-plugin.so json-plugin.so mysql-plugin.so zone-manager-plugin.so streamer.so
hostname ${TITLE}

logtimeformat [%H:%M:%S]

onfoot_rate 32
incar_rate 32
weapon_rate 32
stream_distance 500.0
stream_rate 500
